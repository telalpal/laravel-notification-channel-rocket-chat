{
    "name": "laravel-notification-channels/rocket-chat",
    "description": "Rocket.Chat Notifications channel for Laravel 5.3+",
    "homepage": "https://github.com/laravel-notification-channels/rocket-chat",
    "license": "MIT",
    "keywords": [
        "laravel",
        "notifications",
        "rocketchat",
        "rocket.chat",
        "chat"
    ],
    "authors": [
        {
            "name": "Anton Komarev",
            "email": "anton@komarev.com",
            "homepage": "https://komarev.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/notifications": "^6.2",
        "illuminate/queue": "^6.2",
        "illuminate/support": "^6.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.4",
        "phpunit/phpunit": "~8.4.1"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\RocketChat\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\RocketChat\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
